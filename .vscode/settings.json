{
    "eslint.packageManager": "pnpm",
    "eslint.workingDirectories": [
        "./packages/turbine",
        "./packages/desktop",
        "./libs/typings",
        "./libs/turbine-machines"
    ],
    "eslint.quiet": false,
    "eslint.probe": [
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact",
        "html",
        "markdown",
        "svelte"
    ],
    "eslint.validate": [
        "svelte",
        "js",
        "ts"
    ],
    "i18n-ally.localesPaths": ["libs/misc/i18n/**/*"],
    "i18n-ally.usage.scanningIgnore": ["packages/desktop/build/**/*.js"],
    "i18n-ally.keystyle": "nested",
    "i18n-ally.enabledFrameworks": ["svelte"],
    "css.lint.unknownAtRules": "ignore",
}