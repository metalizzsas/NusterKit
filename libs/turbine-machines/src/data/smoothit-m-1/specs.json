                  "io": {
                     "gateName": "uv#overheat",
                     "gateValue": 0
                  }
               }
            }
         ],
         "steps": [
            {
               "name": "start",
               "isEnabled": 1,
               "blocks": [
                  {"sleep": 30}
               ],
               "startBlocks": [
                  {"io_write": ["air-heater", 1]},
                  {"io_write": ["ventilation", 1]},
                  {"io_write": ["motor#speed", {"multiply": [{"profile": "motor#speed"}, {"profile": "motor#enabled"}]}]},
                  {"if": {
                     "statement": {
                        "comparator": "==",
                        "left_side": {"profile": "motor#reverseEnabled"},
                        "right_side": 1
                     },
                     "true_blocks": [
                        {"start_timer": {
                           "timer_name": "motorRotation",
                           "timer_interval": {"profile": "motor#reverseTime"},
                           "blocks": [{"io_write": ["motor#direction", {"reverse": {"io_read": "motor#direction"}}]}]
                        }}
                     ]
                  }}
               ],
               "endBlocks": []
            },
            {
               "name": "pulverization",
               "isEnabled": {"profile": "pulverization#enabled"},
               "runAmount": {"conditional": {
                  "comparator": "==",
                  "left_side": [{"profile": "multilayer#enabled"}, {"profile": "multilayer#count"}],
                  "right_side": [1, 1]
               }},
               "blocks": [
                  {"sleep": 3},
                  {"if": {
                     "statement": {
                        "comparator": "!=",
                        "left_side": {"io_read": "nozzle#sensor-open"},
                        "right_side": 1
                     },
                     "true_blocks": [{"stop": "nozzle#cache-wrong"}]
                  }},
                  {"if": {
                     "statement": {
                        "comparator": "!=",
                        "left_side": {"io_read": "uv#sensor-closed"},
                        "right_side": 1
                     },
                     "true_blocks": [{"stop": "uv#cache-wrong"}]
                  }},
                  {"if": {
                     "statement": {
                        "comparator": "!=",
                        "left_side": {"io_read": "ir#sensor-closed"},
                        "right_side": 1
                     },
                     "true_blocks": [{"stop": "ir#cache-wrong"}]
                  }},
                  {"for": {
                     "limit": {"profile": "pulverization#count"},
                     "blocks": [
                        {"io_write": ["nozzle#pulverization", 1]},
                        {"sleep": {"profile": "pulverization#timeOn"}},
                        {"io_write": ["nozzle#pulverization", 0]},
                        {"sleep": {"profile": "pulverization#timeOff"}}
                     ]
                  }}
               ],
               "startBlocks": [
                  {"io_write": ["nozzle#cache", 0]},
                  {"io_write": ["uv#cache", 1]},
                  {"io_write": ["ir#cache", 1]},
                  {"io_write": ["resin#pressure", 1]}
               ],
               "endBlocks": [
                  {"io_write": ["resin#pressure", 0]},
                  {"io_write": ["nozzle#cache", 1]}
               ]
            },
            {
               "name": "ir-curing",
               "isEnabled": {"profile": "ir-curing#enabled"},
               "runAmount": {"conditional": {
                  "comparator": "==",
                  "left_side": [{"profile": "multilayer#enabled"}, {"profile": "multilayer#count"}],
                  "right_side": [1, 1]
               }},
               "blocks": [
                  {"sleep": 3},
                  {"if": {
                     "statement": {
                        "comparator": "!=",
                        "left_side": {"io_read": "ir#sensor-open"},
                        "right_side": 1
                     },
                     "true_blocks": [{"stop": "ir#cache-wrong"}]
                  }},
                  {"if": {
                     "statement": {
                        "comparator": "!=",
                        "left_side": {"io_read": "nozzle#sensor-closed"},
                        "right_side": 1
                     },
                     "true_blocks": [{"stop": "nozzle#cache-wrong"}]
                  }},
                  {"if": {
                     "statement": {
                        "comparator": "!=",
                        "left_side": {"io_read": "uv#sensor-closed"},
                        "right_side": 1
                     },
                     "true_blocks": [{"stop": "uv#cache-wrong"}]
                  }},
                  {"io_write": ["ir#enable", 1]},
                  {"sleep": {"profile": "ir-curing#duration"}},
                  {"io_write": ["ir#enable", 0]}
               ],
               "startBlocks": [
                  {"io_write": ["ir#cache", 0]},
                  {"io_write": ["ir#ventil", 1]}
               ],
               "endBlocks": [
                  {"io_write": ["ir#cache", 1]},
                  {"append_maintenance": ["ir-lamp", {"multiply": [0.0002777778, {"profile": "ir-curing#duration"}]}]}
               ]
            },
            {
               "name": "inerting",
               "isEnabled": {"profile": "inerting#enabled"},
               "runAmount": {"conditional": {
                  "comparator": "==",
                  "left_side": [{"profile": "multilayer#enabled"}, {"profile": "multilayer#count"}],
                  "right_side": [1, 1]
               }},
               "blocks": [
                  {"sleep": {"profile": "inerting#duration"}}
               ],
               "startBlocks": [
                  {"io_write": ["inerting", 1]},
                  {"io_write": ["ventilation", 0]}
               ],
               "endBlocks": [
                  {"io_write": ["inerting", 1]}
               ]
            },
            {
               "name": "uv-curing",
               "isEnabled": {"profile": "uv-curing#enabled"},
               "partialStepFallback": 1,
               "runAmount": {"conditional": {
                  "comparator": "==",
                  "left_side": [{"profile": "multilayer#enabled"}, {"profile": "multilayer#count"}],
                  "right_side": [1, 1]
               }},
               "blocks": [
                  {"sleep": 3},
                  {"if": {
                     "statement": {
                        "comparator": "!=",
                        "left_side": {"io_read": "uv#sensor-open"},
                        "right_side": 1
                     },
                     "true_blocks": [{"stop": "uv#cache-wrong"}]
                  }},
                  {"if": {
                     "statement": {
                        "comparator": "!=",
                        "left_side": {"io_read": "ir#sensor-closed"},
                        "right_side": 1
                     },
                     "true_blocks": [{"stop": "ir#cache-wrong"}]
                  }},
                  {"if": {
                     "statement": {
                        "comparator": "!=",
                        "left_side": {"io_read": "nozzle#sensor-closed"},
                        "right_side": 1
                     },
                     "true_blocks": [{"stop": "nozzle#cache-wrong"}]
                  }},
                  {"io_write": ["uv#power", {"profile": "uv-curing#uv-power"}]},
                  {"io_write": ["uv#enable", 1]},
                  {"sleep": {"conditional": {
                     "comparator": "==",
                     "left_side": [{"profile": "multilayer#enabled"}, {"conditional": {
                        "comparator": "==",
                        "left_side": [{"add": [1, {"read_var": "currentStepRunCount"}]}, {"profile": "uv-curing#duration"}],
                        "right_side": [{"profile": "multilayer#count"}, {"profile": "multilayer#interlayer-uvduration"}]
                     }}],
                     "right_side": [1, {"profile": "uv-curing#duration"}]
                  }}},
                  {"io_write": ["uv#power", 0]},
                  {"io_write": ["uv#enable", 0]}
               ],
               "startBlocks": [
                  {"io_write": ["uv#cache", 0]},
                  {"io_write": ["uv#ventil", 1]},
                  {"io_write": ["ventilation", 0]}
               ],
               "endBlocks": [
                  {"io_write": ["uv#cache", 1]},
                  {"append_maintenance": ["uv-lamp", {"multiply": [0.000277778, {"conditional": {
                     "comparator": "==",
                     "left_side": [{"profile": "multilayer#enabled"}, {"conditional": {
                        "comparator": "==",
                        "left_side": [{"add": [1, {"read_var": "currentStepRunCount"}]}, {"profile": "uv-curing#duration"}],
                        "right_side": [{"profile": "multilayer#count"}, {"profile": "multilayer#interlayer-uvduration"}]
                     }}],
                     "right_side": [1, {"profile": "uv-curing#duration"}]
                  }}]}]},
                  {"io_write": ["ventilation", 1]},
                  {"stop_timer": "motorRotation"}
               ]
            },
            {
               "name": "vapors-extraction",
               "isEnabled": 1,
               "runAmount": 1,
               "blocks": [
                  {"sleep": {"profile": "vapors-extraction#duration"}}
               ],
               "startBlocks": [
                  {"io_write": ["air-heater", 0]}
               ],
               "endBlocks": [
                  {"io_write": ["ventilation", 0]},
                  {"io_write": ["motor#speed", 0]},
                  {"io_write": ["nozzle#cache", 0]},
                  {"io_write": ["uv#cache", 0]},
                  {"io_write": ["ir#cache", 0]},
                  {"io_write": ["uv#ventil", 0]},
                  {"io_write": ["ir#ventil", 0]},
                  {"append_maintenance": ["smt-filter", 1]},
                  {"append_maintenance": ["inner-cleaning", 1]}
               ]
            }
