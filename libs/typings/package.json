{
  "name": "@metalizzsas/nuster-typings",
  "version": "1.6.2",
  "description": "Typings for NusterKit",
  "types": "build/index.d.ts",
  "main": "build/index.js",
  "files": [
    "build"
  ],
  "exports": {
    "default": "./build/index.js",
    "types": "./build/index.d.ts"
  },
  "engines": {
    "node": "18.x"
  },
  "scripts": {
    "lint": "eslint --ext .ts .",
    "dev": "tsc --watch",
    "build": "pnpm run schema && tsc",
    "schema": "npm run schema:config && npm run schema:spec && npm run schema:translations",
    "schema:config": "typescript-json-schema src/configuration/index.ts Configuration --noExtraProps --required -o src/schemas/schema-config.json",
    "schema:spec": "typescript-json-schema src/spec/index.ts MachineSpecs --noExtraProps --required -o src/schemas/schema-specs.json",
    "schema:translations": "typescript-json-schema src/translations/index.ts Translations --noExtraProps --required -o src/schemas/schema-translations.json"
  },
  "keywords": [],
  "author": "@metalizzsas/kworz",
  "license": "UNLICENSED",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "eslint": "^8.48.0",
    "typescript": "^5.2.2",
    "typescript-json-schema": "0.60.0"
  },
  "repository": {
    "type": "git",
    "directory": "libs/typings",
    "url": "git+https://github.com/metalizzsas/NusterKit.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "dependencies": {
    "@types/node": "^20.5.7"
  }
}
