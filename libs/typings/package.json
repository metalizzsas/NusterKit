{
  "name": "@metalizzsas/nuster-typings",
  "version": "1.3.0",
  "description": "Typings for NusterKit",
  "types": "build/index.d.ts",
  "main": "build/index.js",
  "engines": {
    "node": "18.x"
  },
  "scripts": {
    "lint": "eslint --ext .js,.ts .",
    "dev": "tsc --watch",
    "build": "tsc && npm run schema:config && npm run schema:spec",
    "schema:config": "typescript-json-schema src/configuration/index.ts IConfiguration --noExtraProps --required -o build/schemas/schema-config.json",
    "schema:spec": "typescript-json-schema src/spec/index.ts IMachineSpecs --noExtraProps --required -o build/schemas/schema-specs.json"
  },
  "keywords": [],
  "author": "@metalizzsas/kworz",
  "license": "UNLICENSED",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "eslint": "^8.24.0",
    "typescript": "^4.8.4",
    "typescript-json-schema": "0.54.0"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  }
}
