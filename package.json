{
  "name": "nuster-turbine",
  "version": "1.0.0",
  "description": "Complete rewrite of NusterEngine in Typescript",
  "main": "app.js",
  "scripts": {
    "dev": "./node_modules/.bin/concurrently --kill-others \"npm run dev:app\" \"npm run dev:modbus\" \"npm run dev:enip\"",
    "dev:app": "nodemon src/app.ts | node_modules/.bin/pino-pretty",
    "dev:modbus": "node dev/modbus.mjs",
    "dev:enip": "node dev/enip.mjs",
    "machine": "export NODE_ENV=\"production\"; nodemon src/app.ts | node_modules/.bin/pino-pretty",
    "docs": "typedoc --out docs --entryPointStrategy expand ./src",
    "build": "tsc",
    "start": "node build/app.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kworz/NusterTurbine.git"
  },
  "keywords": [],
  "author": "Metalizz SAS",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/Kworz/NusterTurbine/issues"
  },
  "homepage": "https://github.com/Kworz/NusterTurbine#readme",
  "dependencies": {
    "deep-extend": "^0.6.0",
    "express": "^4.17.1",
    "modbus-serial": "^8.0.5",
    "mongoose": "^6.0.13",
    "pino": "^7.6.0",
    "pino-http": "^6.5.0",
    "st-ethernet-ip": "^2.4.4",
    "ts-udp-discovery": "^1.0.0",
    "udp-discovery": "^2.0.2",
    "ws": "^8.3.0",
    "nuster-turbine-machines": "git+git@github.com:Kworz/NusterTurbineMachines.git"
  },
  "devDependencies": {
    "@types/deep-extend": "^0.4.32",
    "@types/express": "^4.17.13",
    "@types/lodash": "^4.14.177",
    "@types/node": "^16.11.9",
    "@types/ws": "^8.2.0",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "concurrently": "^6.1.0",
    "eslint": "^8.4.1",
    "nodemon": "^2.0.15",
    "pino-pretty": "^7.3.0",
    "ts-node": "^10.4.0",
    "typedoc": "^0.22.10",
    "typescript": "^4.5.2"
  }
}
