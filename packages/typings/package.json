{
  "name": "@metalizz/nuster-typings",
  "version": "1.0.0",
  "description": "Typings for NusterKit",
  "main": "src/index.js",
  "scripts": {
    "dev": "tsc --watch --noEmit",
    "build": "tsc --noEmit",
    "schema": "npm run schema:config && npm run schema:spec",
    "schema:config": "typescript-json-schema src/configuration/index.ts IConfiguration --noExtraProps --required -o src/schemas/schema-config.json",
    "schema:spec": "typescript-json-schema src/spec/index.ts IMachineSpecs --noExtraProps --required -o src/schemas/schema-specs.json"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "typescript": "^4.8.4",
    "typescript-json-schema": "0.54.0"
  }
}
